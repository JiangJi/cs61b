
public class RBTree {
    private static final boolean RED = true;
    private static final boolean BLACK = false;
    class Node
    {
        int val;
        Node left;
        Node right;
        boolean color;
    }
    private boolean isRed(Node n)
    {
        if(n==null) return false;
        return n.color == RED;
    }
